{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "MsSqlDevelopment": "Data Source=(localdb)\\MSSQLLocalDB;Database=RentACarCourseDb;Trusted_Connection=true"
  },

  "CacheSettings": {
    "SlidingExpiration": 2
  },

  "LogConfigurations": {
    "SerilogConfigurations": {
      "FileLogConfiguration": {
        "FolderPath": "/logs/"
      },
      "MsSqlLogConfiguration": {
        "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Database=RentACarCourseDb;Trusted_Connection=true",
        "TableName": "Logs",
        "AutoCreateSqlTable": true
      }
    }
  },

  "AllowedHosts": "*"
}